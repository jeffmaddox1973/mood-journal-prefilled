{
  "compilerOptions": {
    "allowJs": false,
    "jsx": "react-jsx",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2021",
    "types": [
      "react",
      "react-native"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}